{
  "expo": {
    "name": "apkCDCH",
    "slug": "apkCDCH",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cdchLogo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#f6f3f3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Esta aplicacion usara FaceID para validar el inicio de sesion.",
        "NSCameraUsageDescription": "La aplicación accede a la cámara para poder escanear el codigo QR."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.brisa_garcia.boletaje"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b6670a7d-caa4-4199-8ef1-2ecc7c5987ad"
      }
    }
  }
}