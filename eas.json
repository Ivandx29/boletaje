{
  "cli": {
    "version": ">= 3.15.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "autoIncrement": "versionCode",
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "autoIncrement": "buildNumber",
        "distribution":"store",
        "enterpriseProvisioning": "universal"
        
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "distribution":"store",
        "enterpriseProvisioning": "universal",
        "autoIncrement": "buildNumber",
        "image":"latest",
        "resourceClass": "default"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
